{
  "verifier": {
    "command": "{{exe}} --config \"{{input}}\" --verifier \"{{dll}}\" --out-dir=\"{{out_dir}}\""
  },
  "verifierUnit": {
    "command": "{{exe}} --gtest_filter={{input}}"
  },
  "verifierApi": {
    "command": "{{exe}} --gtest_filter={{input}}"
  },
  "exactoutput": {
    "command": "{{exe}} -binFile:\"{{input}}\" -outFile:\"{{out_file}}\" -onScreen:0 -memoryLimit:4096 -iAmDebugging:1"
  },
  "machsim": {
    "command": "{{exe}} newmode --input-dir \"{{input}}\" --output-dir \"{{out_dir}}\" --tmp-dir \"{{out_dir}}/__tmp\" --stand-alone 1",
    "input_is_dir": true
  },
  "mxp": {
    "command": "{{exe}} newmode --input-dir \"{{input}}\" --output-dir \"{{out_dir}}\""
  },
  "cuttercompensation": {
    "command": "{{exe}} newmode --input-dir \"{{input}}\" --output-dir \"{{out_dir}}\""
  },
  "collisionchecker": {
    "command": "{{exe}} newmode --input-dir \"{{input}}\" --output-dir \"{{out_dir}}\""
  },
  "cncsim": {
    "command": "{{exe}} \"{{input}}\""
  }
}