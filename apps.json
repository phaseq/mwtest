    "alias": [
      "CNCSimTest"
    ],
    "tags": [
      "cncsimtests"
    ],
    "builds": {
      "dev-windows": {
        "exe": "{{dev_dir}}/CNCSim/mwCNCSimTest/bin/{{build_config_skipunicode}}/mwCNCSimTest.exe",
        "cwd": "{{dev_dir}}/5axis/customer/quickstart",
        "solution": "{{dev_dir}}/5axis/build_all_VS2013.sln",
        "project": "mwCNCSimTest"
      },
      "cmake-windows": {
        "exe": "{{build_dir}}/Binaries/build/{{build_config}}/mwCNCSimTest.exe",
