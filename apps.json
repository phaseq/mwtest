      },
      "quickstart": {
        "exe": "{{build_dir}}/mwCNCSimTest.exe",
        "cwd": "{{build_dir}}"
      }
    },
    "tests": {
      "ci": {
        "id_pattern": "cncsim/automated/smoke/(.*)",
        "groups": [
          {
            "find_glob": "cncsim/automated/smoke/**/*.cnclog",
            "timeout": 120,
            "execution_style": "parallel"
          },
          {
            "find_glob": "cncsim/automated/smoke/**/.cnc",
            "timeout": 120,
            "execution_style": "parallel"
          }
        ]
      },
      "ci_disabled": {
        "id_pattern": "cncsim/(.*)",
        "groups": [
          {
            "find_glob": "cncsim/ReferenceFiles/GeneratedCNCFiles/**/*.cnclog",
            "timeout": 120,
            "execution_style": "parallel"
          },
          {
            "find_glob": "cncsim/TestCases/**/*.cnclog",
            "timeout": 120,
            "execution_style": "parallel"
          },
          {
            "find_glob": "cncsim/CNCSimReferenceFiles/**/*.cnclog",
            "timeout": 120,
            "execution_style": "parallel"
          }
        ]
      },
      "custom": {
        "id_pattern": "(.*)",
        "groups": [
          {
            "find_glob": "**/*.cnclog",
            "execution_style": "parallel"
          }
        ]
      }
    }
