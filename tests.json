{
  "verifier": {
    "command_template": [
      "{{mwVerifierTest}}",
      "--config", "{{input_path}}",
      "--verifier", "{{mwVerifierDll}}",
      "--out-dir", "{{tmp_path}}"
    ]
  },
  "verifierUnit": {
    "command_template": [
      "{{mwVerifierUnitTest}}",
      "--gtest_filter", "{{input_path}}"
    ],
    "cwd": "{{mwVerifierUnitTestCWD}}"
  },
  "verifierApi": {
    "command_template": [
      "{{mwVerifierApiTest}}",
      "--gtest_filter={{input_path}}"
    ],
    "cwd": "{{mwVerifierApiTestCWD}}"
  },
  "machsim": {
    "command_template": [
      "{{machsimtest}}", "newmode",
      "--input-dir", "{{input_path}}",
      "--output-dir", "{{tmp_path}}",
      "--tmp-dir", "{{tmp_path}}",
      "--stand-alone", "1"
    ],
    "inputs_are_dirs": true
  },
  "cncsim": {
    "command_template": ["{{mwCNCSimTest}}", "{{input_path}}", "-log", "{{tmp_path}}"],
    "cwd": "{{mwCNCSimTestCWD}}"
  }
}