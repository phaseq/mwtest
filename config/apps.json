{
  "verifier": {
    "command_template": [
      "{{exe}}",
      "--config",
      "{{input}}",
      "--verifier",
      "{{dll}}",
      "--out-dir",
      "{{tmp_path}}"
    ]
  },
  "verifierUnit": {
    "command_template": [
      "{{exe}}",
      "--gtest_filter={{input}}"
    ]
  },
  "verifierApi": {
    "command_template": [
      "{{exe}}",
      "--gtest_filter={{input}}"
    ]
  },
  "machsim": {
    "command_template": [
      "{{exe}}",
      "newmode",
      "--input-dir",
      "{{input}}",
      "--output-dir",
      "{{tmp_path}}",
      "--tmp-dir",
      "{{tmp_path}}/__tmp",
      "--stand-alone",
      "1"
    ],
    "input_is_dir": true
  },
  "cncsim": {
    "command_template": [
      "{{exe}}",
      "{{input}}",
      "-log",
      "{{tmp_path}}"
    ]
  },
  "eo": {
    "command_template": [
      "{{exe}}",
      "-binFile:{{input}}",
      "-outFile:{{tmp_file}}",
      "-onScreen:0",
      "-timeout:0",
      "-memoryLimit:4096",
      "-iAmDebugging:1"
    ]
  },
  "mxp": {
    "command_template": [
      "{{exe}}",
      "newmode",
      "--input-dir",
      "{{input}}",
      "--output-dir",
      "{{tmp_path}}"
    ]
  },
  "cuttercompensation": {
    "command_template": [
      "{{exe}}",
      "newmode",
      "--input-dir",
      "{{input}}",
      "--output-dir",
      "{{tmp_path}}"
    ]
  },
  "collisionchecker": {
    "command_template": [
      "{{exe}}",
      "newmode",
      "--input-dir",
      "{{input}}",
      "--output-dir",
      "{{tmp_path}}"
    ]
  }
}