{
  "verifier": {
    "id_pattern": "cutsim/_servertest/verifier/(.*).verytest.ini",
    "groups": [
      {
        "find_glob": "cutsim/_servertest/verifier/smoke/**/*.verytest.ini"
      },
      {
        "find_glob": "cutsim/_servertest/verifier/nightly/**/*.verytest.ini"
      }
    ]
  },
  "verifierUnit": {
    "id_pattern": "(.*)",
    "groups": [
      {
        "find_gtest": "*-*RunLocal*"
      },
      {
        "find_gtest": "*RunLocal*",
        "xge": false
      }
    ]
  },
  "verifierApi": {
    "id_pattern": "(.*)",
    "groups": [
      {
        "find_gtest": "*-*RunLocal*"
      },
      {
        "find_gtest": "*RunLocal*",
        "xge": false
      }
    ]
  },
  "stockmanager": {
    "id_pattern": "cutsim/_servertest/stockmanager/(.*).ini",
    "groups": [
      {
        "find_glob": "cutsim/_servertest/stockmanager/smoke/**/*.ini"
      },
      {
        "find_glob": "cutsim/_servertest/stockmanager/nightly/**/*.ini"
      }
    ]
  },
  "machsim": {
    "id_pattern": "machsim/MachSimTestCases/(.*)",
    "groups": [
      {
        "find_glob": "machsim/MachSimTestCases/BinTestInputPools/**/report.xml"
      },
      {
        "find_glob": "machsim/MachSimTestCases/LargeTestInputPools/**/report.xml"
      }
    ]
  },
  "cncsim": {
    "id_pattern": "cncsim/(.*)",
    "groups": [
      {
        "find_glob": "cncsim/ReferenceFiles/GeneratedCNCFiles/**/*.cnclog"
      },
      {
        "find_glob": "cncsim/TestCases/**/*.cnclog"
      },
      {
        "find_glob": "cncsim/CNCSimReferenceFiles/**/*.cnclog"
      }
    ]
  },
  "exactoutput": {
    "id_pattern": "5axis/binfiles/(.*).bin",
    "groups": [
      {
        "find_glob": "5axis/binfiles/**/*.bin"
      }
    ]
  },
  "mxp": {
    "id_pattern": "5axis/MXPfiles/(.*).mxp",
    "groups": [
      {
        "find_glob": "5axis/MXPfiles/*.mxp"
      }
    ]
  },
  "cuttercompensation": {
    "id_pattern": "5axis/CutterCompensationTests/(.*).cc",
    "groups": [
      {
        "find_glob": "5axis/CutterCompensationTests/*.cc"
      }
    ]
  },
  "collisionchecker": {
    "id_pattern": "CollisionChecker/CollisionCheckerTestCases/(.*).col",
    "groups": [
      {
        "find_glob": "CollisionChecker/CollisionCheckerTestCases/smoke/**/*.col"
      },
      {
        "find_glob": "CollisionChecker/CollisionCheckerTestCases/LargeTestInputPools/**/*.col"
      }
    ]
  },
  "cadcam": {
    "id_pattern": "(.*)",
    "groups": [
      {
        "find_gtest": "*"
      }
    ]
  }
}